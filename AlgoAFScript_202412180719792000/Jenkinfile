pipeline {
    agent any

    tools {
        // Install the Maven version configured as "M3" and add it to the path.
        maven "M3"
    }

    stages {
        stage('Build') {
            steps {
                // Clone code from a GitHub repository using credentials
                git branch: 'main', 
                    credentialsId: '18e47ef9-30e2-47e8-9bbb-8544d02fe702', // Replace with the credentials ID you set
                    url: 'https://github.com/Cherry9676/java_project.git'

                // Run Maven on a Unix agent
               // sh "mvn -Dmaven.test.failure.ignore=true clean package"

                // To run Maven on a Windows agent, use
                // bat "mvn -Dmaven.test.failure.ignore=true clean package"
            }

          
        }
    }
}
